  apt-get install default-jdk -y(if want to install other version specify that).
  java -version
  apt-install maven(to install maven).
  wget https://get.jenkins.io/war-stable/2.249.2/jenkins.war
  mv jenkins.war /root/
  nohup  java -jar /root/jenkins.war --httpPort=9090 > output.txt & 
  cat /root/.jenkins/secrets/initialAdminPassword
